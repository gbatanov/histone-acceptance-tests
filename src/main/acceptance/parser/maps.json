[{
	"name": "maps like array",
	"cases": [{
		"input": "{{[]}}",
		"expected": [
			[107, []]
		]
	}, {
		"input": "{{[1, '2', 3.14]}}",
		"expected": [
			[107, [
				[null, [101, 1]],
				[null, [103, "2"]],
				[null, [102, 3.14]]
			]]
		]
	}, {
		"input": "{{[}}",
		"exception": {
			"line": "1",
			"expected": "expression",
			"found": "}}"
		}
	}, {
		"input": "{{[1,]}}",
		"exception": {
			"line": "1",
			"expected": "expression",
			"found": "]"
		}
	}, {
		"input": "{{[1,}}",
		"exception": {
			"line": "1",
			"expected": "expression",
			"found": "}}"
		}
	}, {
		"input": "{{[1}}",
		"exception": {
			"line": "1",
			"expected": "]",
			"found": "}}"
		}
	}]
}, {
	"name": "map liek object",
	"cases": [{
		"input": "{{[foo: bar}}",
		"exception": {
			"line": "1",
			"expected": "]",
			"found": "}}"
		}
	}, {
		"input": "{{[]}}",
		"expected": [
			[107, []]
		]
	}, {
		"input": "{{[foo: 1, \"str\": '2', 12: 3.14]}}",
		"expected": [
			[107, [
				["foo", [101, 1]],
				["str", [103, "2"]],
				["12", [102, 3.14]]
			]]
		]
	}, {
		"input": "{{[1,]}}",
		"exception": {
			"line": "1",
			"expected": "expression",
			"found": "]"
		}
	}, {
		"input": "{{[macro: 1}}",
		"exception": {
			"line": "1",
			"expected": "expression",
			"found": "macro"
		}
	}, {
		"input": "{{[1:}}",
		"exception": {
			"line": "1",
			"expected": "expression",
			"found": "}}"
		}
	}, {
		"input": "{{[1:'foo',}}",
		"exception": {
			"line": "1",
			"expected": "expression",
			"found": "}}"
		}
	}]
}]