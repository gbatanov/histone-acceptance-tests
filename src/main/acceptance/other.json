[{
	"name": "all other test cases",
	"cases": [{
		"input": "{{%for x in items}}{{else",
		"expectedException": {
			"line": "1",
			"expected": "%}}",
			"found": "EOF"
		}
	}, {
		"input": "hello {{}} world",
		"expectedAST": ["hello ", " world"]
	}, {
		"input": "a {{% hello {{10 * 2}} world %}} b",
		"expectedAST": ["a ", " hello {{10 * 2}} world ", " b"]
	}, {
		"input": "{{+}}",
		"expectedException": {
			"line": "1",
			"expected": "expression",
			"found": "}}"
		}
	}, {
		"input": "{{\r+}}",
		"expectedException": {
			"line": "2",
			"expected": "expression",
			"found": "}}"
		}
	}, {
		"input": "{{\r\r+}}",
		"expectedException": {
			"line": "3",
			"expected": "expression",
			"found": "}}"
		}
	}, {
		"input": "fragment",
		"expectedAST": ["fragment"]
	}, {
		"input": "fragment1 fragment2",
		"expectedAST": ["fragment1 fragment2"]
	}, {
		"input": "fragment1 {{* comment *}} fragment2",
		"expectedAST": ["fragment1 ", " fragment2"]
	}, {
		"input": "fragment1 {{* comment{{* *}} fragment2",
		"expectedAST": ["fragment1 ", " fragment2"]
	}, {
		"input": "fragment1 {{* comment*}} *}} fragment2",
		"expectedAST": ["fragment1 ", " *}} fragment2"]
	}, {
		"input": "{{* comment without closing block",
		"expectedException": {
			"line": "1",
			"expected": "*}}",
			"found": "EOF"
		}
	}]
}]