[{
	"name": "map.max",
	"cases": [{
		"input": "a {{[].max()}} b",
		"expectedResult": "a  b"
	}, {
		"input": "a {{[].max().isUndefined()}} b",
		"expectedResult": "a true b"
	}, {
		"input": "a {{[-10, -20, 0, 30, 40].max()}} b",
		"expectedResult": "a 40 b"
	}, {
		"input": "a {{['FOO', 30, 0, true, false].max()}} b",
		"expectedResult": "a 30 b"
	}]
}]