[{
	"name": "daysInMonth",
	"cases": [{
			"input": "a {{daysInMonth(2012, 4)}} b",
			"expectedResult": "a 30 b"
	}, {
			"input": "a {{daysInMonth(2012, 3)}} b",
			"expectedResult": "a 31 b"
	}, {
			"input": "a {{daysInMonth(2012, 2)}} b",
			"expectedResult": "a 29 b"
	}, {
			"input": "a {{daysInMonth(1900, 2)}} b",
			"expectedResult": "a 28 b"
	}, {
			"input": "a {{daysInMonth(2011, 2)}} b",
			"expectedResult": "a 28 b"
	}, {
			"input": "a {{daysInMonth(1900, 2).isNumber()}} b",
			"expectedResult": "a true b"
	}, {
			"input": "a {{daysInMonth(1900,0).isUndefined()}} b",
			"expectedResult": "a true b"
	}, {
			"input": "a {{daysInMonth(1900,13).isUndefined()}} b",
			"expectedResult": "a true b"
	}, {
			"input": "a {{daysInMonth(1900).isUndefined()}} b",
			"expectedResult": "a true b"
	}, {
			"input": "a {{daysInMonth('2012',2)}} b",
			"expectedResult": "a 29 b"
	}, {
			"input": "a {{daysInMonth(2012,'2')}} b",
			"expectedResult": "a 29 b"
	}, {
			"input": "a {{daysInMonth('2012','2')}} b",
			"expectedResult": "a 29 b"
	}]
}]