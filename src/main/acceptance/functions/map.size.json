[{
	"name": "map.size",
	"cases": [{
		"input": "a {{[].size()}} b",
		"expectedResult": "a 0 b"
	}, {
		"input": "a {{[2, 3, 4].size()}} b",
		"expectedResult": "a 3 b"
	}, {
		"input": "a {{['a':1,'b':2].size()}} b",
		"expectedResult": "a 2 b"
	}, {
		"input": "a {{[2, 3, 4].size().isNumber()}} b",
		"expectedResult": "a true b"
	}]
}]