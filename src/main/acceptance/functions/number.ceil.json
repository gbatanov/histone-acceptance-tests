[{
	"name": "number.ceil",
	"cases": [{
		"input": "a {{0.60.ceil()}} b",
		"expectedResult": "a 1 b"
	}, {
		"input": "a {{0.40.ceil()}} b",
		"expectedResult": "a 1 b"
	}, {
		"input": "a {{5.ceil()}} b",
		"expectedResult": "a 5 b"
	}, {
		"input": "a {{5.1.ceil()}} b",
		"expectedResult": "a 6 b"
	}, {
		"input": "a {{-5.1.ceil()}} b",
		"expectedResult": "a -5 b"
	}, {
		"input": "a {{-5.9.ceil().isNumber()}} b",
		"expectedResult": "a true b"
	}, {
		"input": "a {{-5.9.ceil().isNumber()}} b",
		"expectedResult": "a true b"
	}]
}]