[{
	"name": "map.min",
	"cases": [{
		"input": "a {{[].min()}} b",
		"expectedResult": "a  b"
	}, {
		"input": "a {{[].min().isUndefined()}} b",
		"expectedResult": "a true b"
	}, {
		"input": "a {{[-10, -20, 0, 30, 40].min()}} b",
		"expectedResult": "a -20 b"
	}, {
		"input": "a {{['FOO', 30, 0, true, false].min()}} b",
		"expectedResult": "a 0 b"
	}]
}]