[{
    "name": "functions",
    "cases": [{
        "input": "{{foo()[1]}}",
        "expectedAST": [
            [105, [
                [106, null, "foo", null],
                [101, 1]
            ]]
        ]
    }, {
        "input": "{{dayOfWeek(2012, 4, 2)}}",
        "expectedAST": [
            [106, null, "dayOfWeek", [
                [101, 2012],
                [101, 4],
                [101, 2]
            ]]
        ]
    }, {
        "input": "{{\"string\"()}}",
        "expectedException": {
            "line": "1",
            "expected": "}}",
            "found": "("
        }
    }, {
        "input": "a {{macro name}} b {{/macro}} c",
        "expectedAST": ["a ", [1003, "name", [],
            [" b "]
        ], " c"]
    }, {
        "input": "a {{macro",
        "expectedException": {
            "line": "1",
            "expected": "}}",
            "found": "EOF"
        }
    }, {
        "input": "a {{macro name",
        "expectedException": {
            "line": "1",
            "expected": "}}",
            "found": "EOF"
        }
    }, {
        "input": "a {{macro name(",
        "expectedException": {
            "line": "1",
            "expected": "identifier",
            "found": "EOF"
        }
    }, {
        "input": "a {{macro name(aa",
        "expectedException": {
            "line": "1",
            "expected": ")",
            "found": "EOF"
        }
    }, {
        "input": "a {{macro name(aa,",
        "expectedException": {
            "line": "1",
            "expected": "identifier",
            "found": "EOF"
        }
    }, {
        "input": "a {{macro name(aa,)",
        "expectedException": {
            "line": "1",
            "expected": "identifier",
            "found": ")"
        }
    }, {
        "input": "a {{macro name()}} b {{/macro}} c",
        "expectedAST": ["a ", [1003, "name", [],
            [" b "]
        ], " c"]
    }, {
        "input": "a {{macro name(arg1, arg2, arg3)}} b {{/macro}} c",
        "expectedAST": ["a ", [1003, "name", ["arg1", "arg2", "arg3"],
            [" b "]
        ], " c"]
    }, {
        "input": "a {{macro name(a,b,c)",
        "expectedException": {
            "line": "1",
            "expected": "}}",
            "found": "EOF"
        }
    }, {
        "input": "a {{macro name(a,b,c)}} bbb",
        "expectedException": {
            "line": "1",
            "expected": "{{/macro}}",
            "found": "EOF"
        }
    }, {
        "input": "{{foo(3.14E+3}}",
        "expectedException": {
            "line": "1",
            "expected": ")",
            "found": "}}"
        }
    }, {
        "input": "{{true.foo(11)}}",
        "expectedAST": [
            [106, [105, [
                [16]
            ]], "foo", [
                [101, 11]
            ]]
        ]
    }, {
        "input": "{{false.foo(11)}}",
        "expectedAST": [
            [106, [105, [
                [17]
            ]], "foo", [
                [101, 11]
            ]]
        ]
    }, {
        "input": "{{null.foo()}}",
        "expectedAST": [
            [106, [105, [
                [100]
            ]], "foo", null]
        ]
    }, {
        "input": "{{this.foo()}}",
        "expectedAST": [
            [106, [105, ["this"]], "foo", null]
        ]
    }, {
        "input": "{{self.foo()}}",
        "expectedAST": [
            [106, [105, ["self"]], "foo", null]
        ]
    }, {
        "input": "{{test.foo(}}",
        "expectedException": {
            "line": "1",
            "expected": "expression",
            "found": "}}"
        }
    }, {
        "input": "{{'string'.foo()}}",
        "expectedAST": [
            [106, [105, [
                [103, "string"]
            ]], "foo", null]
        ]
    }, {
        "input": "{{111.foo(11}}",
        "expectedException": {
            "line": "1",
            "expected": ")",
            "found": "}}"
        }
    }, {
        "input": "{{12.foo()}}",
        "expectedAST": [
            [106, [105, [
                [101, 12]
            ]], "foo", null]
        ]
    }, {
        "input": "{{17.62.foo()}}",
        "expectedAST": [
            [106, [105, [
                [102, 17.62]
            ]], "foo", null]
        ]
    }, {
        "input": "{{17..62.foo()}}",
        "expectedException": {
            "line": "1",
            "expected": "identifier",
            "found": ".62"
        }
    }, {
        "input": "{{test()}}",
        "expectedAST": [
            [106, null, "test", null]
        ]
    }, {
        "input": "{{17(#}}",
        "expectedException": {
            "line": "1",
            "expected": "}}",
            "found": "("
        }
    }, {
        "input": "{{test(1, 'string', 7.14)}}",
        "expectedAST": [
            [106, null, "test", [
                [101, 1],
                [103, "string"],
                [102, 7.14]
            ]]
        ]
    }, {
        "input": "{{foo.test()}}",
        "expectedAST": [
            [106, [105, ["foo"]], "test", null]
        ]
    }, {
        "input": "{{foo.test(\"hey\", \"ho\")}}",
        "expectedAST": [
            [106, [105, ["foo"]], "test", [
                [103, "hey"],
                [103, "ho"]
            ]]
        ]
    }, {
        "input": "{{foo['test'](\"hey\", \"ho\")}}",
        "expectedAST": [
            [106, [105, ["foo"]],
                [103, "test"],
                [
                    [103, "hey"],
                    [103, "ho"]
                ]
            ]
        ]
    }, {
        "input": "{{foo(\"hey\", \"ho\").isNumber()}}",
        "expectedAST": [
            [106, [105, [
                [106, null, "foo", [
                    [103, "hey"],
                    [103, "ho"]
                ]]
            ]], "isNumber", null]
        ]
    }, {
        "input": "{{foo(14)(1, 2, 3)}}",
        "expectedException": {
            "line": "1",
            "expected": "}}",
            "found": "("
        }
    }, {
        "input": "{{foo(14)(1, 2, 3).do}}",
        "expectedException": {
            "line": "1",
            "expected": "}}",
            "found": "("
        }
    }]
}]