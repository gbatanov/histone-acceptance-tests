		"input": "a {{loadText('data:text/foobar;,e3syICogMn19').isUndefined()}} b",
		"expectedResult": "a true b"
	}, {
		"input": "a {{loadText('data:text/plain;charset=UTF-8;,e3syICogMn19').isUndefined()}} b",
		"expectedResult": "a true b"
	}, {
		"input": "a {{loadText('data:text/foobar;charset=UTF-8;,e3syICogMn19').isUndefined()}} b",
		"expectedResult": "a true b"
	}, {
		"input": "a {{loadText('data:text/plain;charset=BLA-BLA;,e3syICogMn19').isUndefined()}} b",
		"expectedResult": "a true b"
	}, {
		"input": "a {{loadText('data:text/foobar;charset=BLA-BLA;,e3syICogMn19').isUndefined()}} b",
