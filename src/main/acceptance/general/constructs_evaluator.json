}, {
	"name": "for statement",
	"cases": [{
		"input": "a {{for x in sfgsag}}{{x}}{{/for}} b",
		"expectedResult": "a  b"
	}, {
		"input": "a {{for x in items}}{{x}}{{/for}} b",
		"context": {
			"items": null
		},
		"expectedResult": "a  b"
	}, {
		"input": "a {{for x in items}}{{x}}{{/for}} b",
		"context": {
			"items": true
		},
		"expectedResult": "a  b"
	}, {
		"input": "a {{for x in items}}{{x}}{{/for}} b",
		"context": {
			"items": false
		},
		"expectedResult": "a  b"
	}, {
		"input": "a {{for x in items}}{{x}}{{/for}} b",
		"context": {
			"items": -10
		},
		"expectedResult": "a  b"
	}, {
		"input": "a {{for x in items}}{{x}}{{/for}} b",
		"context": {
			"items": 0
		},
		"expectedResult": "a  b"
