[{

		"name": "strings",
		"cases": [{
				"input": "a {{'string'[0]}} b",
				"expected": "a s b"
		}, {
				"input": "a {{'string'[1]}} b",
				"expected": "a t b"
		}, {
				"input": "a {{'string'[10]}} b",
				"expected": "a  b"
		}, {
				"input": "a {{'string'[-1]}} b",
				"expected": "a g b"
		}, {
				"input": "a {{'string'[-5]}} b",
				"expected": "a t b"
		}, {
				"input": "a {{'string'[-15]}} b",
				"expected": "a  b"
		}, {
				"input": "a {{'hello world'}} b",
				"expected": "a hello world b"
		}, {
				"input": "a {{\"hello world\"}} b",
				"expected": "a hello world b"
		}, {
				"input": "a {{'hello\\nworld'}} b",
				"expected": "a hello\nworld b"
		}, {
				"input": "a {{\"string\"[3]}} b",
				"expected": "a i b"
		}, {
				"input": "a {{\"string\"[0]}} b",
				"expected": "a s b"
		}, {
				"input": "a {{\"string\"[-150]}} b",
				"expected": "a  b"
		}, {
				"input": "a {{\"string\"[250]}} b",
				"expected": "a  b"
		}, {
				"input": "a {{\"string\"['3']}} b",
				"expected": "a i b"
		}, {
				"input": "a {{\"string\"['0']}} b",
				"expected": "a s b"
		}, {
				"input": "a {{\"string\".length.isUndefined()}} b",
				"expected": "a true b"
		}, {
				"input": "a {{\"string\".size()}} b",
				"expected": "a 6 b"
		}, {
				"input": "a {{\"STRING\".toLowerCase()}} b",
				"expected": "a string b"
		}, {
				"input": "a {{\"string\".toUpperCase()}} b",
				"expected": "a STRING b"
		}]
}, {
		"name": "String.split",
		"cases": [{
				"input": "a {{\"string\".split('')}} b",
				"expected": "a s t r i n g b"
		}, {
				"input": "a {{\"string\".split('').isMap()}} b",
				"expected": "a true b"
		}, {
				"input": "a {{\"string\".split()}} b",
				"expected": "a s t r i n g b"
		}, {
				"input": "a {{\"string\".split('sdafdsafdsaf')}} b",
				"expected": "a string b"
		}, {
				"input": "a {{\"s.t.r.i.n.g\".split('.')}} b",
				"expected": "a s t r i n g b"
		}]
}, {
		"name": "String.charCodeAt",
		"cases": [{
				"input": "a {{\"string\".charCodeAt(3)}} b",
				"expected": "a 105 b"
		}, {
				"input": "a {{\"string\".charCodeAt(0)}} b",
				"expected": "a 115 b"
		}, {
				"input": "a {{\"string\".charCodeAt(-150)}} b",
				"expected": "a  b"
		}, {
				"input": "a {{\"string\".charCodeAt(5)}} b",
				"expected": "a 103 b"
		}, {
				"input": "a {{\"string\".charCodeAt(6)}} b",
				"expected": "a  b"
		}]
}, {
		"name": "String.strip",
		"cases": [{
				"input": "a {{\" test\".strip()}} b",
				"expected": "a test b"
		}, {
				"input": "a {{\"test \".strip()}} b",
				"expected": "a test b"
		}, {
				"input": "a {{\" test \".strip()}} b",
				"expected": "a test b"
		}, {
				"input": "a {{\"\\ntest\\n\".strip(\"\\n\")}} b",
				"expected": "a test b"
		}, {
				"input": "a {{\"\\n\\r\\ftest\\n\".strip(\"\\n\\r\\f\")}} b",
				"expected": "a test b"
		}, {
				"input": "a {{\"\\n\\r\\ttest\\t\\r\\n\".strip()}} b",
				"expected": "a test b"
		}, {
				"input": "a {{\"\\ntest\\r\\f\\n\".strip(\"\\n\\r\\f\")}} b",
				"expected": "a test b"
		}, {
				"input": "a {{\"123t5es5t098\".strip(\"1234567890\")}} b",
				"expected": "a t5es5t b"
		}, {
				"input": "a {{\" 121test212 \".strip('1','2')}} b",
				"expected": "a  121test212  b"
		}, {
				"input": "a {{\"121test3212\".strip('12',3)}} b",
				"expected": "a test3 b"
		}, {
				"input": "a {{\"121test212\".strip('1','2')}} b",
				"expected": "a test b"
		}, {
				"input": "a {{\"121test212\".strip(1,'2')}} b",
				"expected": "a 121test21 b"
		}, {
				"input": "a {{\"121test212\".strip(['1','2'])}} b",
				"expected": "a 121test212 b"
		}, {
				"input": "a {{\"121test212\".strip([1,'2'])}} b",
				"expected": "a 121test212 b"
		}]
}, {
		"name": "String.slice",
		"cases": [{
				"input": "a {{\"test\".slice(0).isString()}} b",
				"expected": "a true b"
		}, {
				"input": "a {{\"test\".slice(100).isString()}} b",
				"expected": "a true b"
		}, {
				"input": "a {{\"test\".slice(-1).isString()}} b",
				"expected": "a true b"
		}, {
				"input": "a {{\"test\".slice(-100).isString()}} b",
				"expected": "a true b"
		}, {
				"input": "a {{\"0123456\".slice(0)}} b",
				"expected": "a 0123456 b"
		}, {
				"input": "a {{\"0123456\".slice(2)}} b",
				"expected": "a 23456 b"
		}, {
				"input": "a {{\"0123456\".slice(100)}} b",
				"expected": "a  b"
		}, {
				"input": "a {{\"0123456\".slice(-2)}} b",
				"expected": "a 56 b"
		}, {
				"input": "a {{\"0123456\".slice(-100)}} b",
				"expected": "a 0123456 b"
		}, {
				"input": "a {{\"0123456\".slice(0,2)}} b",
				"expected": "a 01 b"
		}, {
				"input": "a {{\"0123456\".slice(1,2)}} b",
				"expected": "a 12 b"
		}, {
				"input": "a {{\"0123456\".slice(0,100)}} b",
				"expected": "a 0123456 b"
		}, {
				"input": "a {{\"0123456\".slice(1,100)}} b",
				"expected": "a 123456 b"
		}, {
				"input": "a {{\"0123456\".slice(0,-2)}} b",
				"expected": "a 01234 b"
		}, {
				"input": "a {{\"0123456\".slice(1,-2)}} b",
				"expected": "a 1234 b"
		}, {
				"input": "a {{\"0123456\".slice(1,-100)}} b",
				"expected": "a  b"
		}, {
				"input": "a {{\"0123456\".slice(-4,3)}} b",
				"expected": "a 345 b"
		}, {
				"input": "a {{\"0123456\".slice(-5,-2)}} b",
				"expected": "a 234 b"
		}, {
				"input": "a {{'string'.slice(1220,0)}} b",
				"expected": "a  b"
		}, {
				"input": "a {{'string'.slice(1220,0).isString()}} b",
				"expected": "a true b"
		}, {
				"input": "a {{'string'.slice(0,-10000)}} b",
				"expected": "a  b"
		}, {
				"input": "a {{'string'.slice(0,-10000).isString()}} b",
				"expected": "a true b"
		}]
}, {
		"name": "String.test",
		"cases": [{
				"input": "a {{\"test\".test('[a-z]')}} b",
				"expected": "a true b"
		}, {
				"input": "a {{\"test\".test('22')}} b",
				"expected": "a false b"
		}]
}]